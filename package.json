{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "Reserved",
  "scripts": {
    "prebuild": "rimraf dist",
    "copyfisjs": "@powershell copy -Recurse -Force './src/fisjs.Module' './dist/'",
    "copyDP": "@powershell copy -Recurse -Force './src/_dependencies/DP/src/services/grpc' './dist/_dependencies/DP/src/services/'",
    "build": "nest build && npm run copyDP",
    "copyDLLs": "@powershell copy -Recurse -Force './src/observer.Module/dlls' './dist/observer.Module/'",
    "copyViews": "@powershell copy -Recurse -Force './src/observer.Module/views' './dist/observer.Module/'",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debugtolog": "nest start --debug --watch>>loglog.txt",
    "start:generateTestData": "node dist/test/generateTestData.js",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "fis": "nest build && nest start"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.7",
    "@grpc/proto-loader": "^0.6.9",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^8.2.4",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.2.4",
    "@nestjs/microservices": "^8.4.0",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/websockets": "^8.2.4",
    "axios": "^0.21.1",
    "axios-observable": "^1.3.0",
    "body-parser": "^1.20.2",
    "browser-env": "^3.3.0",
    "edge-js": "^16.6.0",
    "eventsource": "^2.0.2",
    "google-protobuf": "^3.14.0",
    "graceful-fs": "^4.2.8",
    "https": "^1.0.0",
    "jade": "^1.11.0",
    "json-parse-better-errors": "^1.0.2",
    "jsonschema": "^1.4.0",
    "node-fetch": "^2.6.1",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rfdc": "^1.3.0",
    "rimraf": "3.0.2",
    "rxjs": "^7.5.1",
    "socket.io-client": "^2.4.0",
    "usb": "^2.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.9",
    "@types/node-fetch": "^2.6.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "supertest": "6.1.5",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "dll",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
